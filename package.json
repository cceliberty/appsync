    "aws-cdk": "2.99.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "istanbul-badges-readme": "^1.8.5",
    "jest": "^29.5.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.0.4"
  },
  "dependencies": {
    "aws-cdk-lib": "2.99.1",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
