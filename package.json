{
  "name": "appsync-resolver-examples",
  "version": "0.1.0",
  "bin": {
    "appsync-resolver-examples": "bin/appsync-resolver-examples.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:cov": "jest --coverage",
